<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.com.shehacks.seven.seven">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DashboardActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".BaseActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".LeaderboardActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".UserSelectionActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EducationActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".EducationLearnActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".EducationLearnPickActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".RegisterChildActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".DonationActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".CharitySelectionActivity"
            android:label="@string/title_activity_charity_selection"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>